ALGORITHM checkpoint2(VAR arr : ARRAY_OF INTEGER)
VAR
    i:INTEGER;
    j: ARRAY_OF INTEGER[i];
BEGIN
    FOR i FROM 1 TO (len(arr)-1) DO
        j[i]:= arr[i]
        WHILE (i>0 and arr[i-1]> j[i]) DO
            arr[i]:=arr[i-1]
            i:=i-1
        END_WHILE
        arr[i] := j[i]
    END_FOR
END